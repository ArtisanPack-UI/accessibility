# Exclude development and testing files from releases with "export-ignore".

# Version control and CI/CD
/.gitlab export-ignore
/.gitlab-ci.yml export-ignore
/.gitignore export-ignore
/.gitattributes export-ignore

# IDE and editor config
/.idea export-ignore
/.vscode export-ignore
/.editorconfig export-ignore

# Testing files
/tests export-ignore
/phpunit.xml export-ignore
/phpunit.xml.dist export-ignore
/.phpunit.result.cache export-ignore
/src/**/Tests export-ignore
/src/**/*Test.php export-ignore
/src/**/Test.php export-ignore

# Database files
/database export-ignore

# Documentation
/docs export-ignore
/README.md export-ignore
/CHANGELOG.md export-ignore

# Example plugins
/plugins export-ignore

# Code quality and analysis
/.coderabbit.yaml export-ignore
