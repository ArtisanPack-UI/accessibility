{
    "name": "digitalshopfront/accessibility",
    "description": "A package for all accessibility functions for the Digital Shopfront CMS.",
    "type": "library",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Digitalshopfront\\Accessibility\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jacob Martella",
            "email": "me@jacobmartella.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": ">=5.3"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.1",
        "orchestra/testbench": "^10.2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digitalshopfront\\Accessibility\\A11yServiceProvider"
            ],
            "aliases": {
                "A11y": "Digitalshopfront\\Accessibility\\Facades\\A11y"
            }
        }
    }
}
