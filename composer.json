{
  "name": "artisanpack-ui/accessibility",
  "description": "A package for all accessibility functions for ArtisanPack UI.",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "version": "1.1.1",
  "autoload": {
    "psr-4": {
      "ArtisanPackUI\\Accessibility\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jacob Martella",
      "email": "me@jacobmartella.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": ">=5.3"
  },
  "require-dev": {
    "pestphp/pest": "^3.8",
    "pestphp/pest-plugin-laravel": "^3.1",
    "orchestra/testbench": "^10.2"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArtisanPackUI\\Accessibility\\A11yServiceProvider"
      ],
      "aliases": {
        "A11y": "ArtisanPackUI\\Accessibility\\Facades\\A11y"
      }
    }
  }
}
